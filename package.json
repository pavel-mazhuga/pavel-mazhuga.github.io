    "description": "project description",
    "repository": {
        "type": "git",
        "url": "repository here"
    },
    "author": "Pavel Mazhuga",
    "license": "MIT",
    "browserslist": [
        "Android >= 5",
        "IE >= 11",
        "iOS >= 10",
        "Safari >= 10",
        "last 2 Edge versions",
        "last 2 Chrome versions",
        "last 2 ChromeAndroid versions",
        "last 2 Firefox versions",
        "last 2 Opera versions"
    ],
    "scripts": {
        "test": "jest --passWithNoTests",
        "dev": "cross-env NODE_ENV=development webpack --config webpack/webpack.config.dev.js",
        "prod": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
        "prod:modern": "cross-env BUILD_TYPE=modern npm run prod",
        "prod:legacy": "cross-env BUILD_TYPE=legacy npm run prod",
        "build": "npm run prod:legacy && npm run prod:modern",
        "build:sandbox": "cross-env ENV=sandbox npm run build",
        "build:bitrix": "cross-env ENV=bitrix npm run build",
        "watch": "npm run watch:dev:bitrix",
        "watch:dev": "cross-env NODE_ENV=development webpack --watch --browsersync --config webpack/webpack.config.dev.js",
        "watch:dev:bitrix": "cross-env ENV=bitrix npm run watch:dev",
        "watch:prod": "cross-env NODE_ENV=production webpack --watch -p",
        "lint:js": "cross-env NODE_ENV=development eslint --config .eslintrc --ignore-path .eslintignore --fix *.js src/**/*.js",
        "lint": "npm run lint:js",
        "start": "cross-env NODE_ENV=development webpack-dev-server --hot --config webpack/webpack.config.dev.js",
        "subsetting": "node ./subset-fonts.js",
        "bundle-analyzer:legacy": "cd ./node_modules/.cache && open-cli bundle-analyzer-legacy-production.html",
        "bundle-analyzer:modern": "cd ./node_modules/.cache && open-cli bundle-analyzer-modern-production.html",
        "bundle-analyzer": "npm run bundle-analyzer:legacy && npm run bundle-analyzer:modern",
        "ba": "npm run bundle-analyzer"
    },
