{
    "name": "project-name-here",
    "version": "1.0.0",
    "description": "project description",
    "repository": {
        "type": "git",
        "url": "repository here"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development DEBUG=0 webpack -d",
        "debug": "cross-env NODE_ENV=development DEBUG=1 webpack -d",
        "prod": "cross-env NODE_ENV=production DEBUG=0 webpack --mode=production",
        "prod:sandbox": "cross-env NODE_ENV=production DEBUG=0 ENV=sandbox webpack --mode=production",
        "prod:bitrix": "cross-env NODE_ENV=production DEBUG=0 ENV=bitrix webpack --mode=production",
        "watch": "npm run watch-prod",
        "watch-dev": "cross-env NODE_ENV=development DEBUG=0 webpack --watch -d",
        "watch-debug": "cross-env NODE_ENV=development DEBUG=1 webpack --watch -d",
        "watch-prod": "cross-env NODE_ENV=production DEBUG=0 webpack --watch -p",
        "js-lint": "cross-env NODE_ENV=development eslint --config .eslintrc --ignore-path .eslintignore --fix *.js src/**/*.js",
        "css-lint": "cross-env NODE_ENV=development stylelint src/**/*.scss --fix --syntax scss --formatter verbose --ignore-path .stylelintignore --config .stylelintrc",
        "html-lint": "cross-env NODE_ENV=development node ./script.html-lint.js",
        "lint": "npm run js-lint && npm run css-lint && npm run html-lint",
        "start": "cross-env NODE_ENV=development webpack-dev-server --hot --mode=development",
        "server-debug": "cross-env NODE_ENV=development DEBUG=1 webpack-dev-server --hot --inline -d",
        "build": "npm-run-all js-lint css-lint prod html-lint",
        "browserslist": "npx browserslist",
        "explore": "source-map-explorer build/assets/js/app.min.js build/assets/js/app.min.js.map"
    },
    "author": "Pavel Mazhuga",
    "license": "MIT",
    "browserslist": [
        "Android >= 5",
        "IE >= 10",
        "iOS >= 9",
        "Safari >= 10",
        "last 2 Edge versions",
        "last 2 Chrome versions",
        "last 2 ChromeAndroid versions",
        "last 2 Firefox versions",
        "last 2 Opera versions"
