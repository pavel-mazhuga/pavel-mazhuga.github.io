{
    "name": "project-name-here",
    "version": "1.0.0",
    "description": "project description",
    "repository": {
        "type": "git",
        "url": "repository here"
    },
    "author": "Pavel Mazhuga",
    "license": "MIT",
    "browserslist": {
        "legacyBrowsers": [
            "Android >= 5",
            "IE >= 11",
            "iOS >= 10",
            "Safari >= 10",
            "last 2 Edge versions",
            "last 2 Chrome versions",
            "last 2 ChromeAndroid versions",
            "last 2 Firefox versions",
            "last 2 Opera versions"
        ],
        "modernBrowsers": [
            "Android >= 5",
            "iOS >= 10",
            "Safari >= 10.1",
            "last 2 Edge versions",
            "last 2 Chrome versions",
            "last 2 ChromeAndroid versions",
            "last 2 Firefox versions",
            "last 2 Opera versions"
        ]
    },
    "scripts": {
        "test": "jest",
        "dev": "cross-env NODE_ENV=development webpack --config webpack/webpack.config.dev.js",
        "prod": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
        "prod:sandbox": "cross-env ENV=sandbox npm run prod",
        "prod:bitrix": "cross-env ENV=bitrix npm run prod",
        "watch": "npm run watch:dev",
        "watch:dev": "cross-env NODE_ENV=development webpack --watch --config webpack/webpack.config.dev.js",
        "watch:prod": "cross-env NODE_ENV=production webpack --watch -p",
        "lint:js": "cross-env NODE_ENV=development eslint --config .eslintrc --ignore-path .eslintignore --fix *.js src/**/*.js",
        "lint:html": "cross-env NODE_ENV=development node ./script.html-lint.js",
        "lint": "npm run lint:js && npm run lint:html",
        "start": "cross-env NODE_ENV=development webpack-dev-server --hot --config webpack/webpack.config.dev.js",
        "build": "npm-run-all test lint:js prod lint:html",
        "browserslist": "npx browserslist",
        "subsetting": "call ./fonts.subsetting.bat",
        "bundle-analyzer:legacy": "cd ./node_modules/.cache && start bundle-analyzer-legacy-production.html",
        "bundle-analyzer:modern": "cd ./node_modules/.cache && start bundle-analyzer-modern-production.html",
        "bundle-analyzer": "npm run bundle-analyzer:legacy && npm run bundle-analyzer:modern",
        "ba": "npm run bundle-analyzer",
        "lint-staged": "lint-staged"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,md,css,scss,html}": [
            "pretty-quick --staged"
        ],
        "src/**/*.{js,jsx,ts,tsx}": [
            "jest --findRelatedTests"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
